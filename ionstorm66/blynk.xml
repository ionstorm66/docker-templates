<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category>Network:Other Other:</Category>
  <Date>2016-05-03</Date>
  <Support>https://github.com/ionstorm66/docker-blynk</Support>
  <Name>blynk</Name>
  <Changes></Changes>
  <Description>Container for blynk</Description>
  <Project>http://www.blynk.cc//Project>
  <Registry>https://hub.docker.com/r/ionstorm66/blynk/</Registry>
  <Repository>ionstorm66/blynk</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8442</HostPort>
        <ContainerPort>8442</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8443</HostPort>
        <ContainerPort>8443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/blynk</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>v0.15.3</Version>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <Banner>https://raw.githubusercontent.com/ionstorm66/docker-templates/master/ionstorm66/img/blynk-banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/ionstorm66/docker-templates/master/ionstorm66/img/blynk-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>